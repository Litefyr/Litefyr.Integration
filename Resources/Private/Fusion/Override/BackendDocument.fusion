        <Carbon.IncludeAssets:Internal.Assets location='HeadStart' />
        <Carbon.IncludeAssets:Internal.Assets location='Head' />
        <Carbon.IncludeAssets:Internal.Assets location='BodyStart' />
        <Carbon.IncludeAssets:Internal.Assets location='Body' />
